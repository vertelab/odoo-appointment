<odoo>
  <template id="appointment_booking" name="Appointment booking">
    <div class="oe_structure">
      <div class="container">
        <br />
        <center>
          <h3>Appointments</h3>
        </center>
        <br />
        <t t-foreach="appointments" t-as="spots">
          <h4>
            <span t-esc="spots.appointment_id.name" />
          </h4>
        </t>
      </div>
    </div>	
  </template>
</odoo>


